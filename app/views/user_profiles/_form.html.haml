= form_for @user_profile do |f|
  - if @user_profile.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_profile.errors.count, "error")} prohibited this user_profile from being saved:"
      %ul
        - @user_profile.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :publisher_id
    = f.number_field :publisher_id
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :start_date
    = f.date_select :start_date
  .field
    = f.label :bio
    = f.text_area :bio
  .field
    = f.label :favorite_books
    = f.text_field :favorite_books
  .field
    = f.label :phone
    = f.text_field :phone
  .actions
    = f.submit 'Save'
